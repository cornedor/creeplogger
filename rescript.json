{
  "name": "creeplogger",
  "sources": {
    "dir": "src",
    "subdirs": true
  },
  "package-specs": [
    {
      "module": "esmodule",
      "in-source": true
    }
  ],
  "suffix": ".bs.mjs",
  "dependencies": [
    "@rescript/react",
    "@rescript/core",
    "@glennsl/rescript-fetch",
    "rescript-schema",
    "@greenlabs/ppx-rhf",
    "rescript-classnames",
    "@dck/rescript-date-fns"
  ],
  "ppx-flags": [
    "@greenlabs/ppx-rhf/ppx",
    "rescript-schema-ppx/bin"
  ],
  "jsx": {
    "version": 4,
    "mode": "automatic"
  },
  "compiler-flags": [
    "-open RescriptCore",
    "-open RescriptSchema"
  ],
  "gentypeconfig": {
    "language": "typescript",
    "module": "es6",
    "importPath": "relative",
    "shims": {
      "Map": "Map"
    },
    "debug": {
      "all": false
    },
    "exportInterfaces": false
  }
}
