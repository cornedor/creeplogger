{
  "name": "creeplogger",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    },
    {
      "dir": "tests",
      "subdirs": true,
      "type": "dev"
    }
  ],
  "package-specs": [
    {
      "module": "esmodule",
      "in-source": true
    }
  ],
  "suffix": ".bs.mjs",
  "bs-dependencies": [
    "@rescript/react",
    "@rescript/core",
    "@glennsl/rescript-fetch",
    "rescript-schema",
    "@greenlabs/ppx-rhf",
    "rescript-classnames",
    "@dck/rescript-date-fns"
  ],
  "ppx-flags": [
    "@greenlabs/ppx-rhf/ppx",
    "rescript-schema-ppx/bin"
  ],
  "jsx": {
    "version": 4,
    "mode": "automatic"
  },
  "bsc-flags": [
    "-open RescriptCore",
    "-open RescriptSchema"
  ],
  "gentypeconfig": {
    "language": "typescript",
    "module": "es6",
    "importPath": "relative",
    "shims": {
      "Map": "Map"
    },
    "debug": {
      "all": false
    },
    "exportInterfaces": false
  }
}
